{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/gallery-images.json",
      "dest": "/gallery-images.json"
    },
    {
      "src": "/partners/(.*)",
      "dest": "/partners/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}